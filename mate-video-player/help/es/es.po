#: C/totem.xml:30(title)
msgid "<application>Totem Movie Player</application> Manual"
msgstr "Manual del <application>Reproductor de películas Totem</application>"

#: C/totem.xml:32(year)
msgid "2003"
msgstr "2003"

#: C/totem.xml:33(holder)
