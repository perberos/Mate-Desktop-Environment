"mateconftool-2 \\\n"
"--type string \\\n"
"--set /desktop/mate/background/primary_color #<placeholder-1/>"
msgstr ""
"mateconftool-2 \\\n"
"--type string \\\n"
"--set /desktop/mate/background/primary_color #<placeholder-1/>"

