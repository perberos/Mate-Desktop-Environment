"--set /desktop/mate/background/picture_options none"
msgstr ""
"mateconftool-2 \\\n"
"--type string \\\n"
"--set /desktop/mate/background/picture_options none"

#: C/system-admin-guide.xml:560(userinput)
#, no-wrap
msgid ""
"mateconftool-2 \\\n"
"--type string \\\n"
"--set /desktop/mate/background/color_shading_type solid"
msgstr ""
"mateconftool-2 \\\n"
"--type string \\\n"
"--set /desktop/mate/background/color_shading_type solid"

#: C/system-admin-guide.xml:565(replaceable)
msgid "hexadecimal-color"
msgstr "cor-hexadecimal"

#: C/system-admin-guide.xml:563(userinput)
#, no-wrap
