#~ "\n"
#~ "dbus-send --session \\\n"
#~ "\t  --dest=org.freedesktop.PowerManagement \\\n"
#~ "\t  --type=method_call \\\n"
#~ "\t  --print-reply \\\n"
#~ "\t  --reply-timeout=2000 \\\n"
#~ "\t  /org/freedesktop/PowerManagement \\\n"
#~ "\t  org.freedesktop.PowerManagement.SetDpmsMode \\\n"
#~ "\t  string:suspend\n"
