<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE policyconfig PUBLIC
 "-//freedesktop//DTD PolicyKit Policy Configuration 1.0//EN"
 "http://www.freedesktop.org/standards/PolicyKit/1.0/policyconfig.dtd">
<policyconfig>
  <vendor>The MATE Project</vendor>
  <vendor_url>http://www.mate.org/projects/mateconf/</vendor_url>
  <icon_name>mateconf-editor</icon_name>

  <action id="org.mate.mateconf.defaults.set-system">
    <_description>Change MateConf system values</_description>
    <_message>Privileges are required to change MateConf system values</_message>
    <defaults>
      <allow_inactive>no</allow_inactive>
      <allow_active>auth_admin</allow_active>
    </defaults>
  </action>

  <action id="org.mate.mateconf.defaults.set-mandatory">
    <_description>Change MateConf mandatory values</_description>
    <_message>Privileges are required to change MateConf mandatory values</_message>
    <defaults>
      <allow_inactive>no</allow_inactive>
      <allow_active>auth_admin</allow_active>
    </defaults>
  </action>

</policyconfig>
