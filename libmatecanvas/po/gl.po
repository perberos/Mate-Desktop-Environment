msgid "X position"
msgstr "Posición X"

#: ../libmatecanvas/mate-canvas-rich-text.c:227
msgid "Y position"
msgstr "Posición Y"

#: ../libmatecanvas/mate-canvas-rich-text.c:234
msgid "Width"
msgstr "Largura"

#: ../libmatecanvas/mate-canvas-rich-text.c:235
msgid "Width for text box"
msgstr "Largura da caixa de texto"

#: ../libmatecanvas/mate-canvas-rich-text.c:242
msgid "Height"
msgstr "Altura"

#: ../libmatecanvas/mate-canvas-rich-text.c:243
msgid "Height for text box"
msgstr "Altura da caixa de texto"

#: ../libmatecanvas/mate-canvas-rich-text.c:250
msgid "Editable"
msgstr "Editábel"

#: ../libmatecanvas/mate-canvas-rich-text.c:251
msgid "Is this rich text item editable?"
msgstr "É editábel este elemento de texto enriquecido?"

#: ../libmatecanvas/mate-canvas-rich-text.c:258
msgid "Visible"
msgstr "Visíbel"

#: ../libmatecanvas/mate-canvas-rich-text.c:259
msgid "Is this rich text item visible?"
msgstr "É visíbel este elemento de texto enriquecido?"

#: ../libmatecanvas/mate-canvas-rich-text.c:266
msgid "Cursor Visible"
msgstr "Cursor visíbel"

#: ../libmatecanvas/mate-canvas-rich-text.c:267
msgid "Is the cursor visible in this rich text item?"
msgstr "É visíbel o cursor neste elemento de texto enriquecido?"

#: ../libmatecanvas/mate-canvas-rich-text.c:274
msgid "Cursor Blink"
msgstr "Intermitencia do cursor"

