msgid "Number of pixels in the left margin"
msgstr "Número de píxeles da marxe esquerda"

#: ../libmatecanvas/mate-canvas-rich-text.c:362
msgid "Right Margin"
msgstr "Marxe dereita"

#: ../libmatecanvas/mate-canvas-rich-text.c:363
msgid "Number of pixels in the right margin"
msgstr "Número de píxeles da marxe dereita"

#: ../libmatecanvas/mate-canvas-rich-text.c:371
msgid "Indentation"
msgstr "Sangría"

#: ../libmatecanvas/mate-canvas-rich-text.c:372
msgid "Number of pixels for indentation"
msgstr "Número de píxeles para a sangría"
