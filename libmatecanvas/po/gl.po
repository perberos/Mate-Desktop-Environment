msgid "Does the cursor blink in this rich text item?"
msgstr "É intermitente o cursor neste elemento de texto enriquecido?"

#: ../libmatecanvas/mate-canvas-rich-text.c:282
msgid "Grow Height"
msgstr "Altura de crecemento"

#: ../libmatecanvas/mate-canvas-rich-text.c:283
msgid "Should the text box height grow if the text does not fit?"
msgstr "Debe crecer a altura da caixa de texto se o texto non cabe?"

#: ../libmatecanvas/mate-canvas-rich-text.c:290
msgid "Wrap Mode"
msgstr "Modo de axuste"

#: ../libmatecanvas/mate-canvas-rich-text.c:291
msgid "Wrap mode for multiline text"
msgstr "Modo de axuste para texto con múltiples liñas"

#: ../libmatecanvas/mate-canvas-rich-text.c:299
msgid "Justification"
msgstr "Xustificación"

#: ../libmatecanvas/mate-canvas-rich-text.c:300
msgid "Justification mode"
msgstr "Modo de xustificación"

#: ../libmatecanvas/mate-canvas-rich-text.c:308
msgid "Direction"
msgstr "Dirección"

#: ../libmatecanvas/mate-canvas-rich-text.c:309
msgid "Text direction"
msgstr "Dirección do texto"

#: ../libmatecanvas/mate-canvas-rich-text.c:317
msgid "Anchor"
msgstr "Áncora"

