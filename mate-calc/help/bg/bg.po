msgid "Octal"
msgstr "Осмична"

#: base.xml:211(para)
msgid "8"
msgstr ""

#: base.xml:214(para)
msgid "Decimal"
msgstr "Десетична"

#: base.xml:215(para)
msgid "10"
msgstr ""

#: base.xml:218(para)
msgid "Hexadecimal"
msgstr "Шестнайсетична"

