@USE_ONE_BIG_XML_FALSE@	for pofile in $(PO_LOCATIONS); do \
@USE_ONE_BIG_XML_FALSE@		locale=`echo $$pofile | sed "s;$(top_srcdir)/po-locations/\(.*\)\.po;\1;"`;	\
@USE_ONE_BIG_XML_FALSE@		xmllint --noblanks -o Locations.$$locale.xml $$locale/$(libmateweatherlocations_data); \
@USE_ONE_BIG_XML_FALSE@		rm -f $$locale/$(libmateweatherlocations_data);		\
@USE_ONE_BIG_XML_FALSE@		test -d $$locale && rmdir $$locale;			\
@USE_ONE_BIG_XML_FALSE@		if test "x$(COMPRESS_EXT)" = "x.gz"; then		\
@USE_ONE_BIG_XML_FALSE@			gzip --force Locations.$$locale.xml;		\
@USE_ONE_BIG_XML_FALSE@		fi;							\
@USE_ONE_BIG_XML_FALSE@	done &&								\
@USE_ONE_BIG_XML_FALSE@	xmllint --noblanks -o Locations.xml C/$(libmateweatherlocations_data) && \
@USE_ONE_BIG_XML_FALSE@	rm -f C/$(libmateweatherlocations_data) &&				\
@USE_ONE_BIG_XML_FALSE@	test -d C && rmdir C &&						\
@USE_ONE_BIG_XML_FALSE@	if test "x$(COMPRESS_EXT)" = "x.gz"; then			\
@USE_ONE_BIG_XML_FALSE@		gzip --force Locations.xml;				\
@USE_ONE_BIG_XML_FALSE@	fi &&								\
@USE_ONE_BIG_XML_FALSE@	touch $@

check:
	xmllint --valid --noout $(top_srcdir)/data/Locations.xml.in
	$(srcdir)/check-timezones.sh $(srcdir)/Locations.xml.in

### Locations.xml.in rebuild
rebuild-locations: locationdb.sqlite update-locations.py
	$(AM_V_GEN)($(srcdir)/update-locations.py > Locations.xml.in.new && mv Locations.xml.in.new Locations.xml.in) || rm -f Locations.xml.in.new

locationdb.sqlite: build-locationdb.pl major-cities.txt sources/nsd_cccc.txt sources/POP_PLACES.txt sources/US_CONCISE.txt sources/geonames_dd_dms_date_*.txt city-fixups.pl station-fixups.pl
	$(AM_V_GEN)$(srcdir)/build-locationdb.pl

-include $(top_srcdir)/git.mk

# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
