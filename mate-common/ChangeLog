    Added new feature to get PPP/ISDN support:
    
    enum {
    	GLIBTOP_PPP_STATE_UNKNOWN = 0,
    	GLIBTOP_PPP_STATE_HANGUP,
    	GLIBTOP_PPP_STATE_ONLINE
    };
    
    struct _glibtop_ppp
    {
    	u_int64_t	flags,
    		state,			/* GLIBTOP_PPP_STATE		*/
    		bytes_in,		/* GLIBTOP_PPP_BYTES_IN		*/
    		bytes_out;		/* GLIBTOP_PPP_BYTES_OUT	*/
    };
    
